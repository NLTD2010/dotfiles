#!/bin/bash

# ENV Variables
WINE="$HOME/.local/share/lutris/runners/wine/lutris-osu-2-x86_64/bin/wine"
export WINEPREFIX="$HOME/.local/share/lutris-games/osu!/"

launchRPC() {
    if pidof winediscordipcbridge.exe > /dev/null; then
        notify-send "Discord IPC" "RPC fix is already running!"
    else
        ${WINE} /home/$USER/.local/share/lutris-games/osu!/drive_c/osu/winediscordipcbridge.exe &
    fi
}

launchRPC &
