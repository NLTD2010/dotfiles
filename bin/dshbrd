#!/usr/bin/env zsh
# Opens dashboard kids

case $1 in
    "open")
        /home/kizu/.local/bin/eww close-all
        /home/kizu/.local/bin/eww daemon; /home/kizu/.local/bin/eww open-many eww-window profile date player system-info powermenu
        exit 1
    ;;

    "close")
        /home/kizu/.local/bin/eww close-all;
        exit 1
    ;;
esac
